<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DesencriptarController">
   <children>
      <AnchorPane prefHeight="380.0" prefWidth="640.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" prefHeight="77.0" prefWidth="212.0" text="Desencriptar " textAlignment="CENTER" textOverrun="CLIP">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label alignment="TOP_LEFT" contentDisplay="CENTER" layoutX="290.0" layoutY="60.0" prefHeight="35.0" prefWidth="247.0" text="Mensagem" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <TextArea fx:id="txt_mensagem" layoutX="288.0" layoutY="90.0" prefHeight="200.0" prefWidth="343.0" />
            <Button fx:id="btn_desencriptar" layoutX="42.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#desencriptarMsg" prefHeight="25.0" prefWidth="129.0" text="Desencriptar " />
            <Button fx:id="btn_limparDados" layoutX="460.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#limparDados" prefHeight="25.0" prefWidth="129.0" text="Limpar Dados" />
            <ListView fx:id="lista_mensagens" layoutX="23.0" layoutY="90.0" onMouseClicked="#exibirMsgEncriptada" prefHeight="200.0" prefWidth="259.0" />
            <Label alignment="TOP_LEFT" contentDisplay="CENTER" layoutX="138.0" layoutY="294.0" prefHeight="35.0" prefWidth="66.0" text="Senha:" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <PasswordField fx:id="txt_senha" layoutX="216.0" layoutY="299.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
